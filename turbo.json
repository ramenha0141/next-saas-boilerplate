{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"generate": {},
		"build": {
			"dependsOn": ["^generate", "^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env", ".env.*"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"typecheck": {
			"cache": false
		},
		"dev": {
			"dependsOn": ["^generate"],
			"cache": false,
			"persistent": true
		}
	}
}
